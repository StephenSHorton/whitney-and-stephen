{
	"name": "whitney-and-stephen",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build:tailwind": "postcss src/styles/index.css -o src/styles/tailwind.css",
		"watch:tailwind": "chokidar \"src/\" \"tailwind.config.js\" --ignore src/styles/tailwind.css -c \"npm run build:tailwind\" --silent",
		"start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
		"start:react": "react-scripts start",
		"prebuild": "npm run build:tailwind",
		"build": "npm run prebuild && craco build",
		"eject": "react-scripts eject"
	},
	"dependencies": {
		"@craco/craco": "^6.2.0",
		"@react-three/drei": "^7.12.4",
		"@react-three/fiber": "^7.0.7",
		"@types/three": "^0.132.1",
		"chokidar-cli": "^3.0.0",
		"firebase": "^9.1.3",
		"npm-run-all": "^4.1.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-dom-confetti": "^0.2.0",
		"react-router-dom": "^5.3.0",
		"react-scripts": "4.0.3",
		"react-toastify": "^8.0.3",
		"three": "^0.132.2"
	},
	"devDependencies": {
		"@types/react": "^17.0.20",
		"@types/react-dom": "^17.0.9",
		"@types/react-router-dom": "^5.1.8",
		"autoprefixer": "^9.8.6",
		"postcss": "^7.0.36",
		"postcss-cli": "^6.1.3",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.14",
		"typescript": "^4.4.2"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
